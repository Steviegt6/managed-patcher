{
    // THIS IS A CONFIG USED FOR *TESTING*, DELETE THIS AND USE A COPY OF example.config.json!!


    "decompilation": {
        "decompilationEnabled": "true",
        "languageVersion": "CSharp7_3",
        "frameworkVersion": ".NETFramework,Version=v4.5",
        "decompileTasks": [
            "vanilla"
        ],
        "assemblyPaths": {
            "vanilla": ""
        },
        "decompilationPaths": {
            "vanilla": "./src/decompiled/"
        }
    },
    "diffs": {
        "Diff Patched": ["src/decompiled", "src/vanilla-patched"],
        "Diff Mod": ["src/vanilla-patched", "src/modded"]
    },
    "patches": {
        "Patch Patched": ["src/vanilla-patched", "patches/vanilla-patched"],
        "Patch Mod": ["src/modded", "patches/modded"]
    }
}